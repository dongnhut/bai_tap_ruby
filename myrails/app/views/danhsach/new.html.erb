<%= form_tag "/create" do %>

<%= label_tag :name %>
<%= text_field_tag :name, params[:name] %>
<br>
<%= label_tag :GioiTinh %>
<%= text_field_tag :GioiTinh, params[:GioiTinh] %>
<br>
<%= label_tag :Diachi %>
<%= text_field_tag :DiaChi, params[:DiaChi] %>
